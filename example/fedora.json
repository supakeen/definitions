{
  "format": {
    "installer-live": {
      "filename": "installer.iso",
      "packages": {
        "include": [
          "kernel",
          "kernel-modules",
          "kernel-modules-extra",
          "anaconda",
          "anaconda-install-env-deps",
          "anaconda-live",
          "@anaconda-tools",
          "aajohan-comfortaa-fonts",
          "dracut-live",
          "glibc-all-langpacks",
          "livesys-scripts"
        ],
        "exclude": [
          "fcoe-utils",
          "device-mapper-multipath"
        ]
      },
      "systemd": {
        "services": {
          "enable": [
            "livesys.service",
            "livesys-late.service",
            "tmp.mount"
          ]
        }
      }
    },
    "vhd": {
      "filename": "disk.vhd",
      "packages": {
        "include": [
          "@core",
          "chrony",
          "langpacks-en",
          "net-tools",
          "ntfsprogs",
          "libxcrypt-compat",
          "initscripts",
          "glibc-all-langpacks"
        ],
        "exclude": [
          "dracut-config-rescue",
          "geolite2-city",
          "geolite2-country",
          "zram-generator-defaults"
        ]
      },
      "systemd": {
        "services": {
          "enable": [
            "sshd.service"
          ],
          "disable": [
            "proc-sys-fs-binfmt_misc.mount",
            "loadmodules.service"
          ]
        }
      }
    }
  }
}